package Java_progect.hw_001;

public class task002 {
     public static void main(String[] args) {
        for (int i = 1; i <= 1001; i++) {
            boolean simpDigit = true;  
            for (int j = 2; j <= Math.floor(Math.sqrt(i)); j++){
                if ((i % j) == 0) { 
                    simpDigit = false;
                    break;
                }
            }
            if (simpDigit){
                System.out.print(i + " ");
            }
        }
    }

}
